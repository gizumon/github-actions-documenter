{
  "name": "github-actions-documenter",
  "version": "0.0.1",
  "private": true,
  "description": "Auto documentation for Resuable Wrokflow",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "pnpm run build && pnpm run format && pnpm run lint && pnpm run package && pnpm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gizumon/github-actions-documenter.git"
  },
  "keywords": [
    "actions",
    "documentation",
    "readme",
    "reusable workflow",
    "auto"
  ],
  "author": "tomoatsu sekikawa",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.0.3",
    "@types/js-yaml": "^4.0.5"
  },
  "devDependencies": {
    "@jest/globals": "^28.1.3",
    "@types/node": "^18.6.5",
    "@typescript-eslint/parser": "^5.32.0",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^8.21.0",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-jest": "^25.7.0",
    "github": "link:@types/@actions/github",
    "jest": "^27.5.1",
    "js-yaml": "^4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.5",
    "typescript": "^4.7.4"
  }
}
